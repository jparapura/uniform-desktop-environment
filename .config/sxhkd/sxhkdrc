super + shift + Return
	st

F9
	refbar.sh

super + shift + y
	sxiv ~/office/pics/timetable.png &

super + shift + l
	slock

super + shift + x
	dmenu_logout.sh

super + shift + w
	flameshot gui

super + shift + e
	flameshot full -p ~/tmp

XF86AudioRaiseVolume
	amixer set Master 2dB+ unmute; refbar.sh;

XF86AudioLowerVolume
	amixer set Master 2dB- unmute; refbar.sh;

XF86AudioMute
	amixer set Master toggle; refbar.sh;
