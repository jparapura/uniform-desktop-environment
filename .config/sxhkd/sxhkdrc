super + shift + Return
	st

F9
	refbar.sh

super + shift + y
	sxiv ~/office/pics/timetable.png &

super + shift + l
	sleep 1; xset dpms force off; slock

super + shift + x
	dmenu_logout.sh

super + shift + semicolon
    dunstctl action 0

super + shift + slash
    dunstctl close

super + shift + w
	flameshot gui

super + shift + e
	flameshot full -p ~/tmp

XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle
